    			<script type="text/javascript" src="/sh_backmate_theme_adv/static/src/js/user_menu.js"></script>
    			
    			<t t-if="request.env.lang">
    			<t t-foreach="request.env['res.lang'].sudo().search([('code','=',request.env.lang)])" t-as="lang">
    				<t t-if="lang.direction == 'ltr'">
    					<script type="text/javascript" src="/sh_backmate_theme_adv/static/src/js/custom.js"></script>
    				</t>
    				<t t-else="">
    					<script type="text/javascript" src="/sh_backmate_theme_adv/static/src/js/custom_rtl.js"></script>
    				</t>
    			</t>
    			
    			</t>
    			
    			
    			
